{
  "plugins": ["prettier"],
  "extends": ["react-app", "prettier"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "react/display-name": 0,
    "react/jsx-sort-props": 2,
    "react/react-in-jsx-scope": 0,
    "react/prop-types": 0,
    "no-console": [
      2,
      {
        "allow": ["warn", "error"]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
